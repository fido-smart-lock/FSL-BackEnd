{
  "version": 2,
  "builds": [
      {
          "src": "api/main.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "PATCH",
              "DELETE",
              "OPTIONS"
          ]
      }
  ]
}